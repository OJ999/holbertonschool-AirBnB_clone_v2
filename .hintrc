{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "meta-charset-utf-8": "off",
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  }
}